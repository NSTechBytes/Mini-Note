{
  "manifest_version": 3,
  "name": "Mini Control",
  "version": "10.25",
  "description": "A simple Extension which help you to take quick note.",
  "permissions": [
    
    "storage"
    
  ],
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/images/icon16.png",
      "48": "assets/images/icon48.png",
      "128": "assets/images/icon128.png"
    }
  },
  "icons": {
    "16": "assets/images/icon16.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}
